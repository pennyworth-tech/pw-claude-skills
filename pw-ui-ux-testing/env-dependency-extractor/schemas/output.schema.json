{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://pennyworth.dev/schemas/skills/env-dependency-extractor/output.json",
  "title": "Env Dependency Extractor Output Schema",
  "description": "Output schema for env-dependency-extractor skill with cross-service analysis and AISP quality gates",
  "type": "object",
  "required": ["skillName", "version", "timestamp", "status", "trustTier", "output"],
  "properties": {
    "skillName": { "const": "env-dependency-extractor" },
    "version": { "type": "string", "pattern": "^\\d+\\.\\d+\\.\\d+$" },
    "timestamp": { "type": "string", "format": "date-time" },
    "status": { "enum": ["success", "partial", "failed", "skipped"] },
    "trustTier": { "type": "integer", "minimum": 0, "maximum": 3 },
    "output": {
      "type": "object",
      "required": ["targetPath", "analysisType", "summary", "envVars", "dependencies", "externalServices", "crossServiceIssues", "qualityGates"],
      "properties": {
        "targetPath": { "type": "string", "minLength": 1 },
        "analysisType": { "enum": ["extract", "diff", "both"] },
        "summary": {
          "type": "object",
          "required": ["totalEnvVars", "totalDependencies", "totalExternalServices", "totalIssues", "healthScore"],
          "properties": {
            "totalEnvVars": { "type": "integer", "minimum": 0 },
            "totalDependencies": { "type": "integer", "minimum": 0 },
            "totalExternalServices": { "type": "integer", "minimum": 0 },
            "totalIssues": { "type": "integer", "minimum": 0 },
            "healthScore": { "type": "number", "minimum": 0, "maximum": 100 },
            "byService": {
              "type": "object",
              "additionalProperties": {
                "type": "object",
                "properties": {
                  "envVars": { "type": "integer", "minimum": 0 },
                  "dependencies": { "type": "integer", "minimum": 0 },
                  "externalServices": { "type": "integer", "minimum": 0 }
                }
              }
            },
            "secretsFound": { "type": "integer", "minimum": 0 },
            "missingLockFiles": { "type": "integer", "minimum": 0 },
            "versionMismatches": { "type": "integer", "minimum": 0 },
            "note": { "type": "string" }
          }
        },
        "envVars": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["name", "service", "environment", "isSecret"],
            "properties": {
              "name": { "type": "string" },
              "service": { "type": "string" },
              "environment": { "enum": ["production", "test", "development", "all"] },
              "hasDefault": { "type": "boolean" },
              "defaultValue": { "type": "string" },
              "isSecret": { "type": "boolean" },
              "sources": {
                "type": "array",
                "items": { "type": "string" }
              },
              "usedInCode": { "type": "boolean" },
              "category": { "type": "string" }
            }
          }
        },
        "dependencies": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["name", "version", "service", "ecosystem", "type"],
            "properties": {
              "name": { "type": "string" },
              "version": { "type": "string" },
              "service": { "type": "string" },
              "ecosystem": { "enum": ["npm", "pip", "docker", "bun", "other"] },
              "type": { "enum": ["runtime", "dev", "peer", "build"] },
              "pinned": { "type": "boolean" },
              "hasLockFile": { "type": "boolean" }
            }
          }
        },
        "externalServices": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["provider", "purpose", "services"],
            "properties": {
              "provider": { "type": "string" },
              "purpose": { "type": "string" },
              "authMethod": { "type": "string" },
              "envVars": {
                "type": "array",
                "items": { "type": "string" }
              },
              "services": {
                "type": "array",
                "items": { "type": "string" }
              }
            }
          }
        },
        "changes": {
          "type": "object",
          "properties": {
            "baseBranch": { "type": "string" },
            "added": {
              "type": "array",
              "items": {
                "type": "object",
                "required": ["type", "name", "service", "detail"],
                "properties": {
                  "type": { "enum": ["envVar", "dependency", "externalService"] },
                  "name": { "type": "string" },
                  "service": { "type": "string" },
                  "detail": { "type": "string" }
                }
              }
            },
            "removed": {
              "type": "array",
              "items": {
                "type": "object",
                "required": ["type", "name", "service", "detail"],
                "properties": {
                  "type": { "enum": ["envVar", "dependency", "externalService"] },
                  "name": { "type": "string" },
                  "service": { "type": "string" },
                  "detail": { "type": "string" },
                  "breaking": { "type": "boolean" }
                }
              }
            },
            "changed": {
              "type": "array",
              "items": {
                "type": "object",
                "required": ["type", "name", "service", "from", "to"],
                "properties": {
                  "type": { "enum": ["envVar", "dependency", "externalService"] },
                  "name": { "type": "string" },
                  "service": { "type": "string" },
                  "from": { "type": "string" },
                  "to": { "type": "string" },
                  "breaking": { "type": "boolean" }
                }
              }
            }
          }
        },
        "crossServiceIssues": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["type", "severity", "message"],
            "properties": {
              "type": { "enum": ["version-mismatch", "missing-env-var", "orphaned-env-var", "missing-lock-file", "unpinned-dependency", "secret-exposure", "url-inconsistency", "missing-manifest"] },
              "severity": { "enum": ["HIGH", "MEDIUM", "LOW"] },
              "service": { "type": "string" },
              "package": { "type": "string" },
              "message": { "type": "string" },
              "recommendation": { "type": "string" }
            }
          }
        },
        "qualityGates": {
          "type": "object",
          "required": ["envCoverage", "dependencyTracking", "healthScore", "tier", "rulesPassed", "rulesTotal"],
          "properties": {
            "envCoverage": {
              "type": "object",
              "required": ["pass", "actual", "threshold"],
              "properties": {
                "pass": { "type": "boolean" },
                "actual": { "type": "number", "minimum": 0, "maximum": 100 },
                "threshold": { "const": 90 }
              }
            },
            "dependencyTracking": {
              "type": "object",
              "required": ["pass", "actual", "threshold"],
              "properties": {
                "pass": { "type": "boolean" },
                "actual": { "type": "number", "minimum": 0, "maximum": 100 },
                "threshold": { "const": 100 }
              }
            },
            "healthScore": {
              "type": "object",
              "required": ["pass", "actual", "threshold"],
              "properties": {
                "pass": { "type": "boolean" },
                "actual": { "type": "number", "minimum": 0, "maximum": 100 },
                "threshold": { "const": 75 }
              }
            },
            "tier": { "enum": ["gold", "silver", "bronze"] },
            "rulesPassed": { "type": "integer", "minimum": 0, "maximum": 3 },
            "rulesTotal": { "const": 3 }
          }
        },
        "recommendations": {
          "type": "array",
          "items": { "type": "string" }
        }
      }
    },
    "metadata": { "type": "object" },
    "validation": { "type": "object" }
  }
}
